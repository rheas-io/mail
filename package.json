{
  "name": "@rheas/mail",
  "version": "0.0.2",
  "description": "Email handling module of rheas framework",
  "main": "./index.js",
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "rheas",
    "framework",
    "express",
    "laravel",
    "email",
    "server",
    "nodemailer"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "copypackage": "cp -rf package.json build",
    "build": "tsc && npm run copypackage",
    "copypackage_dev": "cp -rf package.json build_dev",
    "build-dev": "tsc --outDir ./build_dev && npm run copypackage_dev"
  },
  "dependencies": {
    "nodemailer": "^6.4.11",
    "@types/nodemailer": "^6.4.0"
  },
  "devDependencies": {
    "@rheas/contracts": "^0.0.2",
    "@rheas/errors": "^0.0.1",
    "@rheas/queue": "^0.0.1",
    "@rheas/services": "^0.0.1",
    "@rheas/support": "^0.0.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "jest": "^25.4.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rheas-io/mail.git"
  },
  "author": "Kalesh Kaladharan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rheas-io/mail/issues"
  },
  "homepage": "https://github.com/rheas-io/mail#readme"
}
